{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    {
      "src": "/(.*\\.(png|jpg|jpeg|svg|gif|ico|css|js|woff|woff2))",
      "dest": "/$1"
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
